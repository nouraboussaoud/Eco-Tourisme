        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/waste-management/2025/1/#email">
        <rdfs:label>email</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/waste-management/2025/1/#dateCreation">
        <rdfs:label>date création</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/waste-management/2025/1/#dateActivite">
        <rdfs:label>date activité</rdfs:label>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/waste-management/2025/1/#Activite"/>
