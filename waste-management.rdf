        <rdfs:domain rdf:resource="http://www.semanticweb.org/waste-management/2025/1/#Points"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/waste-management/2025/1/#quantite">
        <rdfs:label>quantité</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/waste-management/2025/1/#unite">
