        <wm:nom>Déchets Organiques</wm:nom>
        <wm:description>Résidus alimentaires et jardins</wm:description>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/waste-management/2025/1/#TypeRecyclable">
        <rdf:type rdf:resource="http://www.semanticweb.org/waste-management/2025/1/#DechetsRecyclables"/>
        <wm:nom>Déchets Recyclables</wm:nom>
        <wm:description>Papier, plastique, verre, métal</wm:description>
    </owl:NamedIndividual>

