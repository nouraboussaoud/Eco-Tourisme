    <!-- PROPRIÉTÉS DONNÉES (Data Properties) -->
    <!-- ================================ -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/waste-management/2025/1/#nom">
        <rdfs:label>nom</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/waste-management/2025/1/#description">
        <rdfs:label>description</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/waste-management/2025/1/#adresse">
        <rdfs:label>adresse</rdfs:label>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/waste-management/2025/1/#PointCollecte"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

